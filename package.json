{
  "name": "scorespad",
  "version": "1.5.0",
  "description": "Easy way to keep your scores when playing board games",
  "homepage": "https://jcperez-ch.github.io/scorespad",
  "keywords": [],
  "main": "src/index.js",
  "dependencies": {
    "@fortawesome/fontawesome-common-types": "^6.3.0",
    "@fortawesome/fontawesome-svg-core": "^6.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.3.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@material-ui/styles": "^4.11.5",
    "gh-pages": "^5.0.0",
    "history": "^5.3.0",
    "i18next": "^19.9.2",
    "idb-keyval": "^6.2.0",
    "jsqr": "^1.2.0",
    "prop-types": "15.7.2",
    "qrcode": "^1.4.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.18.6",
    "react-router": "^6.8.2",
    "react-router-dom": "^6.8.2",
    "react-scripts": "^5.0.1",
    "reset-css": "^4.0.1",
    "styled-components": "^5.3.8"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "customize-cra": "^1.0.0",
    "eslint": "^8.35.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "react-app-rewired": "^2.2.1",
    "webpack-bundle-analyzer": "^4.8.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jcperez-ch/scorespad.git"
  },
  "scripts": {
    "analyze": "react-app-rewired build --stats && webpack-bundle-analyzer build/bundle-stats.json -m static -r build/bundle-stats.html -O && cp node_modules/jsqr/dist/jsQR.js ./build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-app-rewired --openssl-legacy-provider start",
    "build": "react-app-rewired build && cp node_modules/jsqr/dist/jsQR.js ./build",
    "copy": "cp node_modules/jsqr/dist/jsQR.js ./public",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-app-rewired eject",
    "lint": "eslint src --ext .js --ext .jsx --fix"
  },
  "browserslist": [
    ">1%",
    "not dead",
    "not ie > 0",
    "not ie_mob > 0",
    "not op_mini all"
  ]
}
